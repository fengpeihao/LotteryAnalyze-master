<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fph.lotteryanalyze">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.MainActivity"
            android:launchMode="singleTop">
        </activity>
        <activity
            android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AnalyzeActivity" />
        <activity android:name=".activity.DltAnalyzeActivity" />
        <activity android:name=".activity.SsqBlueBallAnalyzeActivity" />
        <activity android:name=".activity.DataListActivity" />
        <activity android:name=".activity.SsqRedBallAnalyzeActivity" />
        <activity android:name=".activity.OmitActivity" />
        <activity android:name=".activity.OmitLimitActivity" />
    </application>

</manifest>